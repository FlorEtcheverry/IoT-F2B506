#File: getTwitterAccount -- returns the twitterData for the user
#Parameter: User -- [PLACEHOLDER] -- String -- M


PREFIX :<http://www.jaatadia.org/IMT/UV2B506Gr2.owl#>
PREFIX base:<http://www.jaatadia.org/IMT/UV2B506Gr2.owl/Individuals#>

PREFIX rdf:<http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs:<http://www.w3.org/2000/01/rdf-schema#>

PREFIX xsd:<http://www.w3.org/2001/XMLSchema#>
PREFIX xml: <http://www.w3.org/XML/1998/namespace>
PREFIX foaf:<http://xmlns.com/foaf/0.1/>

SELECT ?consumerKey ?consumerSecret ?accessToken ?accesTokenSecret;
WHERE {
	base:%%User%% foaf:holdsAccount ?account.
	?account a :twitter.
	?account :consumerSecret ?consumerSecret.
	?account :accessToken ?accessToken.
	?account :accesTokenSecret ?accesTokenSecret.
	?account :consumerKey ?consumerKey.
}
